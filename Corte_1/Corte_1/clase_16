clase

def main(a,b,*args,**kwargs):
    print(a)
    print(b)
    print(args)        #tupla, valores sobrantes
    print(kwargs)      #toma valores ya identificados, diccionario

if __name__ == "__main__":
    main(1,2,3,3,3,5, j=20, m=10,p=21,k=30-24*3)
    
