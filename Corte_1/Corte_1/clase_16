import random as r
num=r.randint(10,20)
print(num)                #sol un numero

for i in range(5):          # 5 numeros o cuantos quiera
    nume=r.randint(10,20)
    print(nume)

for i in range(0,5):
    numero=r.randint(10,20)
    real=round(r.uniform(10,20),3)    # round=redondear(flotante con 2)     r.uniform(inicio final)
    letra=r.choice("murcielago")      # choise= solicita datos str de cadena establecida
    print(nume,end=" ")
    print(real,end=" ")
    print(letra,'\n')
